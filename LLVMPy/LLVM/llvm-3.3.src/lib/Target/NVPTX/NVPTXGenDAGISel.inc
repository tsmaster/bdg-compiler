virtual bool CheckPatternPredicate(unsigned PredNo) const {
  switch (PredNo) {
  default: llvm_unreachable("Invalid predicate in table?");
  case 0: return (Subtarget.hasGenericLdSt());
  case 1: return (UseF32FTZ) && (do_SQRTF32_PREC==1);
  case 2: return (do_SQRTF32_PREC==1);
  case 3: return (UseF32FTZ);
  case 4: return (Subtarget.hasLDG());
  case 5: return (Subtarget.hasLDU());
  case 6: return (Subtarget.hasAtomRedG32());
  case 7: return (Subtarget.hasAtomRedS32());
  case 8: return (Subtarget.hasAtomRedGen32());
  case 9: return (!Subtarget.hasAtomRedGen32() && Subtarget.hasAtomRedG32());
  case 10: return (Subtarget.hasAtomAddF32());
  case 11: return (doMulWide);
  case 12: return (Subtarget.hasAtomRedG64());
  case 13: return (Subtarget.hasAtomRedS64());
  case 14: return (Subtarget.hasAtomRedGen64());
  case 15: return (!Subtarget.hasAtomRedGen64() && Subtarget.hasAtomRedG64());
  case 16: return ((doFMADF32 && UseF32FTZ));
  case 17: return (doFMADF32);
  case 18: return ((doFMAF32 && UseF32FTZ));
  case 19: return (doFMAF32);
  case 20: return (doFMAF64);
  case 21: return (allowFMA);
  case 22: return ((allowFMA && UseF32FTZ));
  case 23: return (do_DIVF32_PREC==0) && (UseF32FTZ);
  case 24: return (do_DIVF32_PREC==0);
  case 25: return (do_DIVF32_PREC==1) && (UseF32FTZ);
  case 26: return (do_DIVF32_PREC==1);
  case 27: return (Subtarget.reqPTX20()) && (UseF32FTZ);
  case 28: return (Subtarget.reqPTX20());
  case 29: return (UseF32FTZ) && (Subtarget.reqPTX20());
  case 30: return ((doFMAF32AGG && UseF32FTZ));
