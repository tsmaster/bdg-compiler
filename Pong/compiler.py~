def makeprog(ops, fn):
    f = open(fn,"wb")
    for i in range(0, len(ops), 2):
        oplen = ops[i]
        opval = ops[i+1]
        print oplen, opval
        if oplen == 1:
            f.write(struct.pack("B",opval))
        elif oplen == 2:
            f.write(struct.pack("h",opval))
        else:
            print "???"
    f.close()

makeprog(initprog, "init_prog.bdgc")
makeprog(mainprog, "main_prog.bdgc")
